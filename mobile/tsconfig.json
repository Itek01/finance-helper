{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,  // Add this line
    "allowSyntheticDefaultImports": true,  // Optional but recommended
    "jsx": "react-native",
    "moduleResolution": "node",
    "target": "esnext",
    "skipLibCheck": true
  }
}
